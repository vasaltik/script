local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/Seven7-lua/Roblox/refs/heads/main/Librarys/Orion/Orion.lua'))()

local Window = OrionLib:MakeWindow({Name = "Private", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
    Name = "AutoFarm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "Hello!",
    Content = "Script Successfully Loaded.",
    Image = "rbxassetid://4483345998",
    Time = 5
})

local remote = game:GetService("ReplicatedStorage")
    :WaitForChild("Remote")
    :WaitForChild("FromLocalForServer")
    :WaitForChild("Get")
    :WaitForChild("st")

local autoFarmEnabled = false

Tab:AddButton({
    Name = "AutoFarm Strength",
    Callback = function()
        autoFarmEnabled = not autoFarmEnabled
        print("Button pressed. AutoFarm:", autoFarmEnabled)
        
        while autoFarmEnabled and task.wait(0.01) do
            pcall(function()
                remote:FireServer()
            end)
        end
    end    
})

local remote = game:GetService("ReplicatedStorage")
    :WaitForChild("Remote")
    :WaitForChild("FromLocalForServer")
    :WaitForChild("Get")
    :WaitForChild("rk")  

    local autoFarmingEnabled = false

Tab:AddButton({
    Name = "Instant Rank",
    Callback = function() 
    autoFarmingEnabled = not autoFarmingEnabled

     print("Button pressed. AutoFarming:", autoFarmingEnabled)
        
        while autoFarmingEnabled and task.wait(0.01) do
            pcall(function()
                remote:FireServer()
            end)
        end
    end    
})

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Box5 = ReplicatedStorage:WaitForChild("Remote"):WaitForChild("FromServerForLocal"):WaitForChild("Box"):WaitForChild("Box5")

local autoFarminggEnabled = false

Tab:AddButton({
    Name = "AutoOpenBox",
    Callback = function() 
        autoFarminggEnabled = not autoFarminggEnabled
        print("Button pressed. AutoFarmingg:", autoFarminggEnabled) -- Исправлено здесь
        while autoFarminggEnabled and task.wait(0.01) do
            pcall(function()
                Box5:FireServer()
            end)
        end
    end
})
OrionLib:Init()
