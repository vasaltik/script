local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Seven7-lua/Roblox/refs/heads/main/Librarys/Orion/Orion.lua')))()
local Window = OrionLib:MakeWindow({Name = "Солевой Бобер", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
 Name = "АвтоФарм",
 Icon = "rbxassetid://4483345998",
 PremiumOnly = false})
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Seven7-lua/Roblox/refs/heads/main/Librarys/Orion/Orion.lua')))()

Tab:AddToggle({
 Name = "АвтоТапание",
 Default = false,
 Callback = function(Value)
  _G.АвтоТапание = Value
  if Value then
    АвтоТапание()
  end
 end 
})
end
OrionLib:Init()
